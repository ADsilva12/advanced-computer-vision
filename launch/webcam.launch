<launch>

  <!-- grab camera image feed and publish -->
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/video1" />
    <!-- <param name="video_device" value="/dev/video0" /> -->
    <param name="camera_frame_id" value="usb_cam" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_info_url" textfile="$(find eecs432_project)/calibration/external_webcam.yml" />
  </node>

  <!-- run image_proc, for webcam -->
  <!-- NOTE: requires camera calibration file to use any interesting topics -->
  <!-- <node name="image_proc" pkg="image_proc" type="image_proc" ns="usb_cam" >
    <param name="camera_info_url" textfile="$(find eecs432_project)/calibration/external_webcam.yml" />
  </node> -->

</launch>
